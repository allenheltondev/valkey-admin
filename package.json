{
  "name": "valkey-skyscope",
  "productName": "Skyscope",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/frontend",
    "apps/server",
    "apps/metrics",
    "common/*"
  ],
  "main": "apps/frontend/electron.main.js",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "concurrently": "^9.2.0",
