    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.38.0"
  },
  "scripts": {
    "dev": "concurrently \"npm --workspace apps/frontend run dev\" \"npm --workspace apps/server run dev\"",
    "build:server": "npm --workspace apps/server run build",
    "build:frontend": "npm --workspace apps/frontend run build",
    "build:metrics": "npm --workspace apps/metrics run build",
    "build:all": "npm run build:server && npm run build:frontend && npm run build:metrics",
    "package:mac": "npm run build:all && electron-builder --mac --arm64",
    "lint": "eslint ."
  },
